pipeline{
    agent any
    environment{
        name='Sudhansu'
        age=28
        isEmployed=true
    }
    stages{
        stage('Stage-Credentials Fetch')
        {
            steps
            {
                echo "Started Credetial stage..."
                script
                {
                    withCredentials([usernamePassword(
                        credentialsId:'sudhansu_personal'
                        usernameVariable:'USER'
                        passwordVariable:'PASS'
                    )])
                    bat "User name is %USER%"
                    bat "User name is %PASS%"

                }
            }
        }
    }
}
