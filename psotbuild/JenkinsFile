pipeline{
    agent any
    stages
    {
        stage('Paralle Executor'){
        parallel{
        stage('exe-1')
        {
            steps
            {
                            echo "Executing the first job."
                            sleep(5)
            }
        }
        stage('exe-2')
        {
            steps
            {
                            echo "Executing the second job."
                                                        sleep(5)

            }        }
        stage('exe-3')
        {
            steps
            {
                            echo "Executing the third job."
                                                        sleep(5)

            }        }
        }
        }
    }
}